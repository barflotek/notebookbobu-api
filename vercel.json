{
  "version": 2,
  "name": "notebookbobu-api",
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "app/main.py": {
      "maxDuration": 300
    }
  }
}